          MODULE=openmp
         VERSION=7.0.1
          SOURCE=$MODULE-$VERSION.src.tar.xz
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/$MODULE-$VERSION.src
      SOURCE_URL=http://releases.llvm.org/${VERSION}
      SOURCE_VFY=sha256:bf16b78a678da67d68405214ec7ee59d86a15f599855806192a75dcfca9b0d0c
        WEB_SITE=http://openmp.llvm.org/
         ENTERED=20181127
         UPDATED=20181216
            TYPE=cmake
           SHORT="LLVM's implementation of OpenMP"

cat << EOF
The OpenMP subproject of LLVM contains the components
required to build an executable OpenMP program
that are outside the compiler itself.
EOF
