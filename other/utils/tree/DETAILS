          MODULE=tree
         VERSION=1.8.0
          SOURCE=$MODULE-$VERSION.tgz
   SOURCE_URL[0]=http://mama.indstate.edu/users/ice/$MODULE/src
   SOURCE_URL[1]=ftp://mama.indstate.edu/linux/$MODULE
      SOURCE_VFY=sha256:
        WEB_SITE=http://mama.indstate.edu/users/ice/tree
         ENTERED=20020906
         UPDATED=20181120
           SHORT="Recursive directory listing"

cat << EOF
Tree is a recursive directory listing program that produces a depth indented
listing of files, which is colorized ala dircolors if the LS_COLORS environment
variable is set and output is to tty.
EOF
