          MODULE=lsof
         VERSION=4.93.2
           SOURCE=$MODULE-$VERSION.tar.gz
 SOURCE_URL_FULL=https://github.com/lsof-org/lsof/archive/$VERSION.tar.gz
      SOURCE_VFY=sha256:3df912bd966fc24dc73ddea3e36a61d79270b21b085936a4caabca56e5b486a2
